<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0       http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
    <kcfgfile name=""/>
    <group name="">
        <entry name="FollowType" type="Int">
            <default>0</default>
        </entry>
        <entry name="FollowRadius" type="Int">
            <default>16</default>
        </entry>
        <entry name="FollowWindowClass" type="String">
            <default>org.kde.dolphin,zen-browser-bin</default>
        </entry>
        <entry name="FollowOffsetX" type="Int">
            <default>-32</default>
        </entry>
        <entry name="FollowOffsetY" type="Int">
            <default>-32</default>
        </entry>
        <entry name="VirtualDesktopBehaviour" type="Int">
            <default>0</default>
        </entry>
        <entry name="VirtualDesktopsAllowed" type="String">
            <default>all</default>
        </entry>
        <entry name="FollowSpeed" type="Int">
            <default>15</default>
        </entry>
        <entry name="IdleTimeout" type="Int">
            <default>1000</default>
        </entry>
        <entry name="SleepTimeout" type="Int">
            <default>10</default>
        </entry>
        <entry name="Appearance" type="Int">
            <default>0</default>
        </entry>
        <entry name="AnimationInterval" type="Int">
            <default>150</default>
        </entry>
        <entry name="FixedPositionX" type="Int">
            <default>0</default>
        </entry>
        <entry name="FixedPositionY" type="Int">
            <default>0</default>
        </entry>
        <entry name="UseUserSprite" type="Bool">
            <default>0</default>
        </entry>
        <entry name="UserSprite" type="Url">
            <default/>
        </entry>
        <entry name="EasterEgg" type="Bool">
            <default>0</default>
        </entry>
    </group>
</kcfg>
